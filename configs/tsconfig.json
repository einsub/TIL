{
 "compilerOptions": {
   "module": "commonjs",
   "moduleResolution": "node",
   "pretty": true,
   "sourceMap": true,
   "target": "es6",
   "outDir": "./dist",
   "baseUrl": "./lib",
   "strictNullChecks": true,
   "typeRoots": [
     "./lib/@types/",
     "./node_modules/@types/"
   ]
 },
 "include": [
   "lib/**/*.ts"
 ],
 "exclude": [
   "node_modules"
 ]
}
